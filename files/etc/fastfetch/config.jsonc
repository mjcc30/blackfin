      "type": "command",
      "key": "Sensei",
      "text": "sensei --version || echo 'Active'"
    },
    "display",
    "de",
    "wm",
    "terminal",
    "cpu",
    "gpu",
    "memory",
    "swap",
    "disk",
    "localip",
    "battery",
    "poweradapter",
    "break",
    "colors"
  ]
}
